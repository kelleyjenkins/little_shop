<div class="analytics-dashboard">
  <h1>Analytics Dashboard</h1> <br>
    <div class="analytics-display">
      <p>User with most orders:<p> <strong><%= User.most_orders_placed %></strong>
      <% @categories.each do |category| %>
      <p>Highest priced item for <%="#{category.title}"%>:</p> <strong><%= category.highest_priced_item %></strong>
      <p>Number of Orders for: <%="#{category.title}"%>:</p> <strong><%= category.orders_placed %></strong>
      <% end %>
    </div><br><br>

  <%= link_to "Admin Dashboard", admin_dashboard_path %>
</div>
